{
  "name": "starter-docker-node-typescript-etc",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "connect-flash": "^0.1.1",
    "express": "^4.17.1",
    "express-list-endpoints": "^4.0.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "mysql": "^2.17.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-github2": "^0.1.11",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.6.1",
    "typeorm": "^0.2.22",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "npx ts-node src/app.ts",
    "develop": "npx ts-node-dev src/app.ts",
    "test": "jest",
    "eslint": "npx eslint --ext .ts src/",
    "migration:generate": "npx ts-node $(npm bin)/typeorm migration:generate",
    "migration:run": "npx ts-node $(npm bin)/typeorm migration:run",
    "migration:revert": "npx ts-node $(npm bin)/typeorm migration:revert"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint"
    }
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-flash": "^0.0.35",
    "@types/express": "^4.17.2",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "^0.0.45",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^24.0.25",
    "@types/mysql": "^2.15.8",
    "@types/node": "^13.1.6",
    "@types/passport-github": "^1.1.5",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2": "^1.4.8",
    "@types/pug": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
